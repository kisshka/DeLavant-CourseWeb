@model List<Course>
@{
    ViewData["Title"] = "Курсы";
}

<div class="content-70percent-width">

@foreach (var course in Model)
{
    <div class="card-item">
        <span class="card-item-name">
            @course.Name
        </span>
        <a class="card-item-open" asp-action="Start" asp-route-id="@course.Id">Начать</a>
    </div>
}
</div>